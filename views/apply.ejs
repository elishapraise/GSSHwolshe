<form action="/students/enroll" class="appointment-form ftco-animate" enctype="multipart/form-data">
    <div class="d-md-flex">
        <div class="form-group">
            <input type="text" name="firstName" class="form-control" placeholder="First Name">
            <p><noscript></noscript>{{firstName}}</p>

        </div>
        <div class="form-group ml-md-4">
            <input type="text" name="lastName" class="form-control" placeholder="Last Name">
            <p>{{lastName}}</p>
        </div>
    </div>
    <div class="d-md-flex">
        <div class="form-group">
            <input type="text" name="parentName" class="form-control" placeholder="Parent FullName">
            <p>{{parentName}}</p>
        </div>
        <div class="form-group ml-md-4">
            <input type="tel" name="phone" class="form-control" placeholder="Parent PhoneNumber">
            <p>{{ParentphoneNumber}}</p>
        </div>
    </div>
    <div class="d-md-flex">
        <div class="form-group ml-md-4">
            <input type="text" name="email" class="form-control" placeholder="Email">
            <p>{{email}}</p>
        </div>


        <div class="form-group ml-md-4">
            <input type="text" name="address" class="form-control" placeholder="Address">
            <p>{{address}}</p>
        </div>

    </div>
    <div class="form-group d-md-flex">

        <input type="submit" value="Apply" class="btn btn-primary btn-sm my-2 p-2">
    </div>
</form>